{
  "name": "surf-app",
  "main": "server.js",
  "version": "0.1.0",
  "engines": {
    "node": "9.4.0",
    "npm": "5.6.0"
  },
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "express": "^4.16.2",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "react-scripts": "^1.1.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
